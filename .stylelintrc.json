{
    "extends": ["stylelint-config-recommended", "stylelint-config-recess-order"],
    "rules": {
        "at-rule-no-unknown": [true, {
            "ignoreAtRules": ["mixin"]
        }],
        "no-descending-specificity": [true, {
            "ignore": ["selectors-within-list"]
        }]
    }
}
